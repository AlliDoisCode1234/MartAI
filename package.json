    "server:restart": "powershell -ExecutionPolicy Bypass -File ./scripts/server-restart.ps1",
    "convex:dev": "convex dev",
    "convex:codegen": "convex codegen",
    "convex:deploy": "convex deploy",
    "prebuild": "npm run convex:codegen || echo 'Warning: Convex codegen skipped - ensure NEXT_PUBLIC_CONVEX_URL is set'",
    "build:vercel": "npm run convex:codegen && npm run build",
    "deploy": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy.ps1",
    "deploy:win": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy.ps1",
    "deploy:unix": "bash ./scripts/deploy.sh"
